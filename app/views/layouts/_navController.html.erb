<div id="logo">
<%= link_to "Logged in as #{current_user.p_fname} #{current_user.p_lname} > Sign Out?", destroy_user_session_path, method: :delete, id: "logged_in_as" %>
</div>
<div class="headerControlsWrapper">
	<div class="header_logo_wrapper">
	<a href="/pages/about"><img id="site_header_logo" src="https://s3.amazonaws.com/gaicd10rails/icd10logowhite.png" /></a>
	</div>
	<div class="menu_item" id="headerOverview">
		<div class="app_icon_container" id="overviewIcon">
			<%= link_to "Task Overview", tasks_path, class: "menu_item_text" %>
		</div>
	</div>
	<div class="menu_item" id="headerIssuesOverview">
		<div class="app_icon_container" id="issuesIcon">
			<%= link_to "Report Issues", issues_path, class: "menu_item_text" %>
		</div>
	</div>
	<div class="menu_item" id="headerHelp">
		<div class="app_icon_container" id="helpIcon">
			<%= link_to "Help Me!", pages_help_path, class: "menu_item_text" %>
		</div>
	</div>
	<div class="menu_item" id="headerAccountModify">
		<div class="app_icon_container" id="accountModifyIcon">
			<%= link_to "Modify Account", edit_user_registration_path, class: "menu_item_text" %>
		</div>
	</div>
	<div class="menu_item" id="headerSignOut">
		<div class="app_icon_container" id="signOutIcon">
			<%= link_to "Sign out?", destroy_user_session_path, method: :delete, class: "menu_item_text" %>
		</div>
	</div>
</div>